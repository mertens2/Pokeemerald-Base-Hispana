
script PlayersHouse_1F_EventScript_WatchGymBroadcast {
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_WalkInPlaceFasterUp)
	waitmovement (0)
	setspeaker (Entrevistador)
	msgbox (PlayersHouse_1F_Text_ReportFromPetalburgGym, MSGBOX_DEFAULT)
	fadedefaultbgm
	special (TurnOffTVScreen)
	setflag (FLAG_SYS_TV_HOME)
	delay (35)
	return
}

script PlayersHouse_1F_EventScript_StarterSet {
	poryswitch(LANGUAGE){
		ES{
			dynmultichoice (25, 4, TRUE, 4, 2, DYN_MULTICHOICE_CB_NONE, "Kanto", "Johto", "Hoenn", "Sinnoh", "Unova", "Kalos", "Alola", "Galar", "Ninguno")
		}
		EN{
			dynmultichoice (25, 4, TRUE, 4, 2, DYN_MULTICHOICE_CB_NONE, "Kanto", "Johto", "Hoenn", "Sinnoh", "Unova", "Kalos", "Alola", "Galar", "None of these")
		}
	}
	addvar(VAR_RESULT, 1)
	if (var(VAR_RESULT) == 9) {
		goto(PlayersHouse_1F_EventScript_StarterWantHardest)
	}
	copyvar(VAR_STARTER_REGION, VAR_RESULT)
	goto (PlayersHouse_1F_EventScript_StarterSetEnd)
}
	
script PlayersHouse_1F_EventScript_StarterSetEnd {
	setspeaker (Mama)
	call (PlayersHouse_1F_EventScript_StarterSet_TellProfessor)
	closemessage
	
	return
}


script PlayersHouse_1F_EventScript_StarterChoseHardest {
	setvar(VAR_STARTER_REGION, 9)
	goto(PlayersHouse_1F_EventScript_StarterSetEnd)
}


