	applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkInPlaceFasterUp)
	applymovement(LOCALID_WINONA, Common_Movement_WalkInPlaceFasterDown)
	waitmovement (0)
	namebox (Winona)
	msgbox(format("Tú eres {PLAYER}, ¿No?\pAquí no soy la Líder del Gimnasio, y no tengo el privilegio de jugar limpio."))
	// multi_fixed_2_vs_1 (TRAINER_WINONA_AQUA_HIDEOUT, "{SHAKE_SCREEN 2 2 3 1}¡AAAHHH! ¡Mi cabeza!", TRAINER_MAXIE_DOUBLE, TRAINER_BACK_PIC_RED)
	// specialvar (VAR_RESULT, GetBattleOutcome)
	// compare (VAR_RESULT, B_OUTCOME_LOST)
	goto (MagmaHideout_4F_EventScript_WinonaDefeated)
}

