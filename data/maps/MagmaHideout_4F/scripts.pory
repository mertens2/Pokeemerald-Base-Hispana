const LOCALID_GROUDON 		   = 1
const LOCALID_GRUNT_1 		   = 2
const LOCALID_GRUNT_2 		   = 3
const LOCALID_GRUNT_3 		   = 4
const LOCALID_TABITHA 		   = 5
const LOCALID_MAXIE 		   = 6
const LOCALID_GROUDON_SLEEPING = 7
const LOCALID_WINONA 		   = 9

script MagmaHideout_4F_Sign_GroudonSleepingPeacefully {
	if (!flag(FLAG_GROUDON_AWAKENED_MAGMA_HIDEOUT)){
		poryswitch(LANGUAGE) {
			EN: msgbox(format("Groudon is still sleeping, rather peacefully, in fact.\pYou wonder if it'd be worth it to sleep for the millenia... It looks relaxing...\pNo! You have family and friends! You gotta make sure they sleep for the millenia with you, too!"))
			ES: msgbox(format("Groudon aún duerme, se ve bastante tranquilo.\pTe preguntas si valdría la pena dormir para la posteridad... Se ve  relajante...\p¡No! ¡Tienes familia! ¡Asegúrate de que también te acompañen en tu sueño primero!"))
		}
	}
	else {
		poryswitch(LANGUAGE) {
			EN: msgbox(format("Groudon was awakened. You can't lose time! You gotta do something!\p... ...\nBut you also can´t help but feel bad for the guy. It was sleeping so peacefully..."))
			ES: msgbox(format("Groudon fue despertado. ¡No pierdas tiempo! ¡Haz algo!\p... ...\nPero no puedes evitar sentir lástima por él. Estaba durmiendo tan tranquilo..."))
		}
	}
	closemessage
}

script MagmaHideout_4F_EventScript_Winona {
	lockall
	special (SpawnCameraObject)
	playbgm (MUS_ENCOUNTER_MAGMA, FALSE)
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_WalkInPlaceFasterUp)
	waitmovement(0)
	namebox (Tabitha)
	msgbox(format("¡Jefe! ¡Es {SHAKE_SCREEN 2 3 2 1}ella!\p¡La líder de la División Delta!"))
	namebox (Winona)
	msgbox(format("Hola, Maxie, ¡Tanto tiempo!\n¿Tienes un momento?\p¡M{SHAKE_SCREEN 6 6 2 3}MMGGGHAA{SHAKE_SCREEN 2 8 2 0}AAAAHHH!"))
	namebox (Maxie)
	msgbox(format("¡Winona! ¿Estás-{PAUSE 30}?"),MSGBOX_INTERRUPT)
	namebox(Winona)
	msgbox(format("Ugh- ¿¡No puedes echar a tu estúpido Admin para tener una conversación normal!?"))
	closemessage
	hidenamebox
	playse(SE_PIN)
	applymovement(LOCALID_MAXIE, Common_Movement_ExclamationMark)
	applymovement(LOCALID_TABITHA, Common_Movement_QuestionMark)
	waitmovement (0)
	namebox (Winona)
	msgbox(format("Está... {PAUSE 30}Empeorando... {PAUSE 30}...{PAUSE 30}\p... ... ...\pDiscúlpame, Maxie..."))
	namebox (Maxie)
	msgbox(format("Winona...{PAUSE 30}\pDetén esta locura, y descansa. No puedes seguir así...\pY no te preocupes, aliviaré el dolor del mundo.\pSólo déjame despertar a Groudon,\ny-{PAUSE 30} "), MSGBOX_INTERRUPT)
	namebox (Winona)
	msgbox(format("{SHAKE_SCREEN 6 6 2 2}¡NO!\p¡Tus soluciones no han funcionado jamás, {SHAKE_SCREEN 3 3 1 0}¿Qué te hace pensar que lo harán ahora, Maxie?\p¿No estás satisfecho con los problemas que causaste entonces?"))
	closemessage
	hidenamebox
	special(RemoveCameraObject)
	if (var(VAR_TEAM_MAGMA_SIDE) > VAR_TEAM_AQUA_SIDE){
		msgbox(format("{COLOR PLAYER}(Parece que están por combatir. ¿Deseas ayudar a Maxie?)"),MSGBOX_YESNO)
		if (var(VAR_RESULT) == YES){
			addvar(VAR_TEAM_MAGMA_SIDE, 1)
			namebox(Maxie)
			msgbox(format("Quédate fuera de esto, Tabitha."))
			applymovement(LOCALID_TABITHA, Common_Movement_WalkDown)
			waitmovement (0)
			applymovement(LOCALID_TABITHA, Common_Movement_WalkInPlaceUp)
			applymovement(LOCALID_MAXIE, Common_Movement_WalkLeft)
			waitmovement(0)
			goto(MagmaHideout_4F_EventScript_AgainstWinona)
		}
		else {
			subvar(VAR_TEAM_MAGMA_SIDE, 1)
			namebox(Maxie)
			msgbox(format("¡Vamos, Tabitha! ¡Con todo!"))
			applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkInPlaceFasterRight)
			waitmovement (0)
			goto(MagmaHideout_4F_EventScript_LetThemFightM)
		}
	}
	else {
		msgbox(format("{COLOR PLAYER}(Parece que están por combatir. ¿Deseas ayudar a Winona?)"),MSGBOX_YESNO)
		if (var(VAR_RESULT) == YES){
			subvar(VAR_TEAM_MAGMA_SIDE, 1)
			applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkInPlaceFasterRight)
			waitmovement (0)
			// goto(MagmaHideout_4F_EventScript_WithWinona)
			goto(MagmaHideout_4F_EventScript_AgainstWinona)
		}
		else {
			addvar(VAR_TEAM_MAGMA_SIDE, 1)
			goto(MagmaHideout_4F_EventScript_LetThemFightM)
		}
	}
	
}


script MagmaHideout_4F_EventScript_LetThemFightM {
	applymovement (OBJ_EVENT_ID_CAMERA, Common_Movement_WalkUp)
	waitmovement (0)
	delay (8)
	namebox(Winona)
	msgbox(format("¡No tengo tiempo para esto!\p¡Cada segundo que perdamos, {COLOR BLUE}él{COLOR DARK_GRAY}..."))
	playse(SE_PIN)
	applymovement(LOCALID_MAXIE, Common_Movement_ExclamationMark)
	waitmovement(0)
	namebox(Winona)
	msgbox(format("Je... Siempre igual...\p{SHAKE_SCREEN 3 3 3 4}¡GRAAHH!"))
	applymovement(LOCALID_WINONA, MagmaHideout_4F_Movement_WinonaAttack)
	applymovement(LOCALID_MAXIE, MagmaHideout_4F_Movement_MaxieJumpBack)
	playse(SE_PIN)
	applymovement(LOCALID_TABITHA, Common_Movement_ExclamationMark)
	waitmovement(0)
	delay (16)
	applymovement(LOCALID_TABITHA, MagmaHideout_4F_Movement_TabithaWorry)
	waitmovement(0)
	namebox(Maxie)
	msgbox("Ouughh...")
	closemessage
	hidenamebox
	playse(SE_PIN)
	applymovement(LOCALID_MAXIE, Common_Movement_ExclamationMark)
	applymovement(LOCALID_MAXIE, MagmaHideout_4F_Movement_MaxieLookAround)
	waitmovement(0)
	playse(SE_PIN)
	applymovement(LOCALID_MAXIE, Common_Movement_ExclamationMark)
	waitmovement(0)
	playse(SE_PIN)
	applymovement(LOCALID_TABITHA, Common_Movement_ExclamationMark)
	waitmovement(0)
	namebox(Tabitha)
	msgbox("No me diga que...")
	hidenamebox
	closemessage
	applymovement(LOCALID_MAXIE, MagmaHideout_4F_Movement_MaxieRun)
	applymovement(LOCALID_TABITHA, MagmaHideout_4F_Movement_TabithaOutWay)
	delay (16)
	fadescreen(FADE_TO_WHITE)
	delay(8)
	namebox(Winona)
	msgbox(format("{TEXT_SPEED 3}Con mi {COLOR BLUE}{PLAY_SE SE_PIN}Flora {COLOR RED}Bis{COLOR GREEN}mu{COLOR BLUE}to{COLOR DARK_GRAY}... {TEXT_SPEED 1} ¡Resolveré todo!"))
	closemessage
	hidenamebox
	delay(8)
	applymovement (LOCALID_WINONA, Common_Movement_SetInvisible)
	waitmovement (0)
	delay (16)
	fadescreen(FADE_FROM_WHITE)
	waitmovement(0)
	applymovement(LOCALID_MAXIE, MagmaHideout_4F_Movement_MaxieLookAround)
	applymovement(LOCALID_TABITHA, MagmaHideout_4F_Movement_MaxieLookAround)
	waitmovement(0)
	applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkUp)
	waitmovement(0)
	applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_FaceRight)
	waitmovement(0)
	namebox(Maxie)
	applymovement (LOCALID_TABITHA, Common_Movement_FacePlayer)
	msgbox(format("{TEXT_SPEED 3}Winona se lo llevó... {COLOR RED}{TEXT_SPEED 1}El Orbe Rojo{COLOR DARK_GRAY}...\pCon el poder que tiene ahora y esa {TEXT_SPEED 5}“Flora Bismuto”{TEXT_SPEED 1} que mencionó...\p¡El resultado podría ser catastrófico!\p¡Hay que avisarle a Archie inmediatamente! ¡Quizá con él, y el poder del {COLOR BLUE}Pokémon Legendario{COLOR DARK_GRAY}, podamos ganarle..."))
	applymovement (LOCALID_MAXIE, Common_Movement_FacePlayer)
	msgbox(format("Deberías ir a buscar a Archie en su Base Secreta..."))
	closemessage
	hidenamebox
	fadescreen(FADE_TO_BLACK)
	removeobject (LOCALID_MAXIE)
	removeobject (LOCALID_TABITHA)
	clearflag (FLAG_HIDE_SLATEPORT_CITY_CAPTAIN_STERN)
	clearflag (FLAG_HIDE_SLATEPORT_CITY_GABBY_AND_TY)
	setvar (VAR_SLATEPORT_CITY_STATE, 1)
	setvar (VAR_LAVARIDGE_TOWN_STATE, 3)
	// setflag (FLAG_GROUDON_AWAKENED_MAGMA_HIDEOUT)
	setvar (VAR_SLATEPORT_HARBOR_STATE, 1)
	fadescreen (FADE_TO_BLACK)
	removeobject (LOCALID_GRUNT_1)
	removeobject (LOCALID_GRUNT_2)
	removeobject (LOCALID_GRUNT_3)
	setflag (FLAG_HIDE_MAGMA_HIDEOUT_GRUNTS)
	delay (16)
	fadescreen(FADE_FROM_BLACK)
	closemessage
	releaseall
}
