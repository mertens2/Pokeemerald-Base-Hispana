	waitmovement (0)
	namebox (Maxie)
	msgbox(format("¿Será así, eh? ¡Se arrepentirán de esto!"))
	// multi_2_vs_2 (TRAINER_MAXIE_DOUBLE, "¡No puede ser!\n¡Hay que proteger el Orbe!",TRAINER_TABITHA_MAGMA_HIDEOUT, "¡S-señor!\n¿Lo tiene usted, ¡verdad!?", TRAINER_MAXIE_DOUBLE, TRAINER_BACK_PIC_RED)
	// specialvar (VAR_RESULT, GetBattleOutcome)
	// compare (VAR_RESULT, B_OUTCOME_LOST)
	goto (MagmaHideout_4F_EventScript_LetThemFightM)
	return
}

movement MagmaHideout_4F_Movement_WinonaAttack {
	jump_in_place_right
	walk_faster_right * 2
	lock_facing_direction
	walk_slow_left
	walk_slow_left
	unlock_facing_direction
	face_left
}

movement MagmaHideout_4F_Movement_MaxieJumpBack {
	lock_facing_direction
	jump_2_right
}

movement MagmaHideout_4F_Movement_TabithaWorry {
	face_up
	walk_faster_right
	walk_faster_up
	walk_in_place_faster_right
	walk_in_place_faster_left
	walk_in_place_faster_right
}

movement MagmaHideout_4F_Movement_TabithaOutWay {
	delay_8
	face_up
	lock_facing_direction
	jump_down
	unlock_facing_direction
	walk_in_place_faster_left
}

movement MagmaHideout_4F_Movement_MaxieRun {
	jump_in_place_left
	walk_faster_left * 2
}

script MagmaHideout_4F_EventScript_DoGroudonAwakening {
	delay (20)
	setvar (VAR_RESULT, 1)
	playse (SE_M_DETECT)
	dofieldeffectsparkle (18, 42, 0)
	waitfieldeffect (FLDEFF_SPARKLE)
	setvar (VAR_RESULT, 0)
	playfanfare (MUS_AWAKEN_LEGEND)
	playse (SE_ORB)
	special (DoOrbEffect)
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_WalkInPlaceFasterRight)
	applymovement (LOCALID_WINONA, Common_Movement_WalkInPlaceFasterRight)
	applymovement (LOCALID_WINONA, Common_Movement_ExclamationMark)
	waitmovement (0)
	delay (150)
	removeobject (LOCALID_GROUDON_SLEEPING)
	addobject (LOCALID_GROUDON)
	waitstate
	delay (60)
	applymovement (LOCALID_GROUDON, MagmaHideout_4F_Movement_GroudonApproach)
	waitmovement (0)
	special (FadeOutOrbEffect)
	waitstate
	setvar (VAR_0x8004, 1)  // vertical pan
	setvar (VAR_0x8005, 1)  // horizontal pan
	setvar (VAR_0x8006, 8)  // num shakes
	setvar (VAR_0x8007, 5)  // shake delay
	special (ShakeCamera)
	waitstate
	applymovement (LOCALID_GROUDON, MagmaHideout_4F_Movement_GroudonExit)
	waitmovement (0)
	removeobject (LOCALID_GROUDON)
	delay (4)
	setvar (VAR_0x8004, 2)  // vertical pan
	setvar (VAR_0x8005, 2)  // horizontal pan
	setvar (VAR_0x8006, 8)  // num shakes
	setvar (VAR_0x8007, 5)  // shake delay
	special (ShakeCamera)
	waitstate
	delay (30)
	applymovement (LOCALID_MAXIE, MagmaHideout_4F_Movement_MaxieLookAround)
	delay (8)
	applymovement (LOCALID_TABITHA, MagmaHideout_4F_Movement_MaxieLookAround)
	delay (16)
	applymovement (LOCALID_WINONA, MagmaHideout_4F_Movement_MaxieLookAround)
	waitmovement (0)
	applymovement (LOCALID_WINONA, Common_Movement_WalkInPlaceFasterRight)
	applymovement (LOCALID_MAXIE, Common_Movement_WalkInPlaceFasterLeft)
	applymovement (LOCALID_TABITHA, Common_Movement_WalkInPlaceFasterLeft)
	waitmovement (0)
	delay (8)
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_WalkInPlaceFasterUp)
	waitmovement (0)
	namebox(Winona)
	msgbox(format("¡¿Porqué lo hiciste?!\n¿No sabes lo que acabas de desatar en Hooenn?\p¿Has olvidado lo que pasó la última vez?"))
	namebox(Maxie)
	msgbox(format("¿Crees que haría algo sin informarme?\NTengo el Orbe Rojo en mi poder, por lo que Groudon sucumbirá a él y recobrará su poder primigenio."
	"\pCon ese poder, daré a la human{PAUSE 5}-{PAUSE 30} "))
	namebox(Winona)
	msgbox(format("¡IDIOTA!\n¡El Orbe hace a Groudon imposible de controlar!\pCon él, planeaba modificar nuestra realidad..."
	"\pMe daría el poder de arreglarlo todo... ¡Y LO ARRUINASTE!"))
	applymovement (LOCALID_MAXIE, Common_Movement_WalkInPlaceFasterLeft)
	namebox(Maxie)
	msgbox(format("¡¿Estás loca!? ¿Modificar la realidad?\N¡Eso es imposible!"))
	applymovement (LOCALID_WINONA, Common_Movement_WalkInPlaceLeft)
	waitmovement (0)
	namebox(Winona)
	msgbox(format("Con mi Flora Bismuto no lo es...\pPero necesita más poder. Ahora, tendré que arriesgarme con sólo el Orbe Azul, ADEMÁS de lidiar con Groudon, ¡Todo gracias a tu ignorancia!"))
	fadescreen(FADE_TO_WHITE)
	waitmovement (16)
	applymovement (LOCALID_WINONA, Common_Movement_SetInvisible)
	applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkUp)
	applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkInPlaceFasterRight)
	waitmovement(0)
	delay (16)
	fadescreen(FADE_FROM_WHITE)
	playse(SE_PIN)
	applymovement(LOCALID_MAXIE, Common_Movement_ExclamationMark)
	applymovement(LOCALID_MAXIE, Common_Movement_FacePlayer)
	namebox(Maxie)
	msgbox(format("¡Archie!\p¡Él seguro ya encontró a Kyogre!\p{PLAYER}, si lo que dice Winona es cierto... ¡Hay que detenerla a ella y a Archie!\pYo saldré a controlar a Groudon."))
	hidenamebox
	fadescreen(FADE_TO_BLACK)
	removeobject (LOCALID_MAXIE)
	removeobject (LOCALID_TABITHA)
	clearflag (FLAG_HIDE_SLATEPORT_CITY_CAPTAIN_STERN)
	clearflag (FLAG_HIDE_SLATEPORT_CITY_GABBY_AND_TY)
	setvar (VAR_SLATEPORT_CITY_STATE, 1)
	setflag (FLAG_GROUDON_AWAKENED_MAGMA_HIDEOUT)
	setvar (VAR_SLATEPORT_HARBOR_STATE, 1)
	fadescreen (FADE_TO_BLACK)
	removeobject (LOCALID_GRUNT_1)
	removeobject (LOCALID_GRUNT_2)
	removeobject (LOCALID_GRUNT_3)
	setflag (FLAG_HIDE_MAGMA_HIDEOUT_GRUNTS)
	delay (16)
	fadescreen(FADE_FROM_BLACK)
	closemessage
	releaseall
}
script MagmaHideout_4F_EventScript_WinonaDefeated {
	special (SpawnCameraObject)
	namebox(Winona)
	msgbox(format("UGHHH... El dolor sigue fuerte...\p¿Porqué... te has entrometido?\pAcaso..."))
	hidenamebox
	closemessage
	delay(8)
	applymovement(OBJ_EVENT_ID_CAMERA, MagmaHideout_4F_Movement_PanToMaxie)
	waitmovement(0)
	goto(MagmaHideout_4F_EventScript_DoGroudonAwakening)
}

movement MagmaHideout_4F_Movement_PanToMaxie {
	walk_diag_northeast
	walk_right
}

