text MauvilleCity_EvoMart_Text_AzurillGirl {
	poryswitch(LANGUAGE) {
		EN {
			format("The Move Tutors in the middle specialize in some Move categories,\N"
			"but the ones in their own rooms teach you moves customized for each Pokémon!")
		}
		ES {
			format("Los Tutores del medio se especializan en categorías específicas,\N"
			"¡Pero los que tienen sus propios cuartos enseñan ataques personalizados para cada Pokémon!")
		}
	}
}

text MauvilleCity_EvoMart_Text_HereToBuy {
	poryswitch(LANGUAGE) {
		EN {
			format("This city is unbelievable!\pFour Move Tutors in one place?\pThis city never ceases to amaze!")
		}
		ES {
			format("¡Esta ciudad es increíble!\p¿Cuatro Tutores de Movimientos en una tienda?\p¡Nunca me dejo de sorprender aquí!")
		}
	}
}

text MauvilleCity_EvoMart_Text_PledgeMoveTutor {
	poryswitch(LANGUAGE) {
		EN {
			format("I'm the Pledge Tutor!\pFor just two of any Colored Shard {COLOR LIGHT_GRAY SHADOW WHITE}but Yellow Shards ARE the best{COLOR DARK_GRAY SHADOW LIGHT_GRAY}, I can teach your Pokémon an Pledge Move!\pWould you like to try?")
		}
		ES {
			format("¡Soy el Tutor de Votos!\pPor dos Partes de cualquier color {COLOR LIGHT_GRAY SHADOW WHITE}aunque las mejores son las Amarillas{COLOR DARK_GRAY SHADOW LIGHT_GRAY}, ¡Le enseñaré a tus Pokémon un Movimiento Voto!\p¿Intentamos?")
		}
	}
}

text MauvilleCity_EvoMart_Text_UltimateMoveTutor {
	poryswitch(LANGUAGE) {
		EN {
			format("I'm the Ultimate Tutor!\pFor just five of any Colored Shard other than Yellow, I can teach your Pokémon an Ultimate Move!\pWould you like to try?")
		}
		ES {
			format("¡Soy la Tutora Suprema!\pPor cinco Partes de cualquier color menos amarillo, ¡Le enseñaré a tus Pokémon un Movimiento Supremo!\p¿Intentamos?")
		}
	}
}

text MauvilleCity_EvoMart_Text_EggMoveTutor {
	poryswitch(LANGUAGE) {
		EN {
			format("Hi, I'm the renowed Egg Move Tutor!\p"
					"For just 10 Green Shards I can teach your Pokemon an Egg Move,\N"
					"and don't worry, evolutions can learn their previous stage's Egg Moves all the same!\p"
					"Wanna try?")
		}
		ES {
			format("¡Hola, soy la reconocida Tutora de Movimientos Huevo!\p"
					"Por 10 Partes Verdes puedo enseñarle un Movimiento Huevo a tu Pokémon,\N"
					"¡Y no te preocupes, si es una evolución podré enseñarle ataques de etapas anteriores!\p"
					"¿Quieres probar?")
		}
	}
}

