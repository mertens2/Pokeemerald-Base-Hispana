text MauvilleCity_Inside_Text_OldMan1{
	poryswitch(LANGUAGE) {
		EN {
			format("Nowadays,{PAUSE 8} the {COLOR BLUE}Residential Area{COLOR DARK_GRAY} is in the second floor,\N"
