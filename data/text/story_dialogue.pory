text SlateportCity_SternsShipyard_1F_Text_CantMakeHeadsOrTails {
	poryswitch(LANGUAGE) {
		EN{
			format("Umm… If this goes here, and that\n"
			"goes over there…\p"
			"Then where does this thing go?\n"
			"Arghh! This man and his doctor handwriting!")
		}
		ES{
			format("A ver… Si esto va aquí y aquello va allá…\p"
			"¿Dónde va esto entonces?\n¡Ahhh! ¡Ese tipo y su letra de doctor!")
		}
	}
}


text SlateportCity_SternsShipyard_1F_Text_MeetDockDeliverToStern {
	poryswitch(LANGUAGE) {
		EN{
			format("Hm?\nHi, I'm Dock Muéllez.\p"
			"Captain Stern commissioned me to design a ferry.\p"
			"Oh! That there…\nAren't those some Devon Goods?\p"
			"Must be for him, the guy really needs them, couldn't stop mumbling about it.\p"
			"Wanna go give them to him? He must be on the Oceanic Museum searching for inspiration.")
		}
		ES{
			format("¿Eh?\nAh, hola, soy Dock Muéllez.\p"
			"El Capitán Stern me ha encargado que diseñe un ferry.\p"
			"Oh, eso de ahí…\n¿Son las Partes Devon?\p"
			"Deben ser para él. El tipo las NECESITA, no paraba de murmurarlo.\p"
			"¿Puedes ir a buscarlo? Creo que andaba por el Museo Océanico buscando inspiración.")
		}
	}
}


text SlateportCity_SternsShipyard_1F_Text_CouldYouFindStern {
	poryswitch(LANGUAGE) {
		EN{
			format("Hm? “Where could Captain Stern have gone off to?”\p"
			"He's probably at the Museum, can you go and deliver that parcel to him?")
		}
		ES{
