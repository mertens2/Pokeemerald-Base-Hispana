text SlateportCity_SternsShipyard_1F_Text_FerryIsReady {
	poryswitch(LANGUAGE) {
		EN{
			format("The ferry is finally ready!\pThe new S.S. Tidal is truly a marvel\n"
			"of technology!\p"
			"But, I get the feeling that we can\n"
			"make something even better.\p"
			"You know, there's never an end to\n"
			"technology's march.")
		}
		ES{
			format("¡El ferry está listo!\p"
			"¡El nuevo S.S. MAREA es toda una maravilla tecnológica!\p"
			"Sin embargo, creo que aún podemos mejorar el diseño.\p"
			"¡Ya sabes que el avance de la ciencia es imparable!")
		}
	}
}


text SlateportCity_SternsShipyard_1F_Text_DecidedToHelpDock {
	poryswitch(LANGUAGE) {
		EN{
			format("MR. BRINEY: Ah, {PLAYER}{KUN}!\n"
			"It's been too long!\p"
			"Aye, since I met you, this old sea dog's\n"
			"been feeling frisky!\p"
			"So I've decided to help DOCK make\n"
			"a ferry.\p"
			"Aye, after all, a ferry would be able\n"
			"to carry a lot of people.\p"
			"But, you know, that DOCK is really\n"
			"something special.\p"
			"With his knack for technology and\n"
			"my experience, I'm sure that we can\l"
			"build one great ship, aye!")
		}
		ES{
			format("¡Anda, {PLAYER}{KUN}!\n¡Cuánto tiempo!\p"
			"Desde que te conocí, me he\n"
			"sentido la mar de inquieto.\p"
			"Así que he decidido ayudar a\n"
			"MUÉLLEZ a construir el ferry.\p"
			"¡Imagina cuánta gente podría\n"
			"transportar en cada viaje!\p"
			"¿Sabes? Este MUÉLLEZ es\n"
			"un tipo especial de verdad.\p"
			"Uniendo sus conocimientos\n"
			"y mi experiencia, haremos\l"
			"un gran barco, ¡ya lo creo!")
		}
	}
}


text SlateportCity_SternsShipyard_1F_Text_SeaIsLikeLivingThing {
	poryswitch(LANGUAGE) {
		EN{
			format("The seasons, the weather, where\n"
			"the moon sits in the sky…\p"
			"These and other conditions make\n"
			"the sea change its expression.\p"
			"That's right!\n"
			"The sea is like a living thing!")
		}
		ES{
			format("Las estaciones, el tiempo,\n"
			"la posición de la Luna…\p"
			"El estado del mar se ve afectado\n"
			"por todo esto y mucho más.\p"
			"¡Exactamente!\n"
			"¡El mar tiene vida!")
		}
	}
}


